{
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true
  },
  "exclude": ["node_modules"]
}
